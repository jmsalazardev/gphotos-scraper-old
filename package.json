{
  "name": "gphotos-scraper",
  "description": "A google photos scraper",
  "license": "MIT",
  "author": {
    "name": "Jos√© Miguel Salazar",
    "email": "contacto@jmsalazar.dev"
  },
  "version": "0.1.0",
  "main": "bin/index.js",
  "bin": "bin/index.js",
  "scripts": {
    "audit": "npx lerna run audit --stream",
    "audit:depcheck": "npx lerna run audit:depcheck --stream",
    "audit:npm-check-updates": "npx npm-check-updates --errorLevel 1",
    "npm:upgrade": "npx npm-check-updates -iu",
    "build": "tsc",
    "create": "npm run build && npm run test",
    "local": "npm i -g && gphotos-scraper",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "start:sync": "ts-node src/index sync https://photos.app.goo.gl/gZ1jq1L16cTenTwT7",
    "lint": "npx tslint -p tsconfig.json -c tslint.json",
    "format": "npx prettier --write \"./src/**/*.ts\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "axios": "^0.26.1",
    "chalk": "^5.0.1",
    "class-transformer": "^0.5.1",
    "clear": "^0.1.0",
    "commander": "^9.1.0",
    "mime-types": "^2.1.35",
    "path": "^0.12.7",
    "puppeteer": "^13.5.2",
    "reflect-metadata": "^0.1.13",
    "tslint": "^6.1.3",
    "typedi": "^0.10.0",
    "typescript-tslint-plugin": "^1.0.2",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/clear": "^0.1.2",
    "@types/figlet": "^1.5.4",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^16.0.0",
    "@types/puppeteer": "^5.4.5",
    "depcheck": "^1.4.3",
    "husky": "^7.0.4",
    "lerna": "^4.0.0",
    "lint-staged": "^12.3.7",
    "nodemon": "^2.0.15",
    "prettier": "2.6.2",
    "prisma": "^3.12.0",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "keywords": [
    "cli",
    "nodejs",
    "gphotos-scraper",
    "typescript"
  ],
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
